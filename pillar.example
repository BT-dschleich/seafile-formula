seafile:
  server:
    config:
      seahub_port: "8000"
    css: |
      a, a:hover, #header .nav .a:hover, #ls-ch a {
        color: #eb8205;
      }
    logo_source: https://www.seafile.com/media/img/logo.png
    logo_hash: sha1=b3c4a3a618ac8e59ba4702503f4e8d1d4dba6d66
    seahub_settings:
      TIME_ZONE: Europe/Berlin
      ADD_REPLY_TO_HEADER: True
    ccnet:
      General:
        SERVICE_URL: http://seafile.example.com
    seafile:
      quota:
        default: 2
      history:
        keep_days: 30
      zip:
        windows_encoding: iso-8859-1
